		<separator/>
		<item label="Feh">
			<action name="Execute">
				<execute>sh ~/.fehbg</execute>
			</action>
		</item>
		<separator/>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
